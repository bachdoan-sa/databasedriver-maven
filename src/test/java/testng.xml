            <parameter name = "myExpectedName" value="manisha"/>
            <classes>
                <class name = "org.teamswt301.dirverdatabasedata.dao.test.ExamParametersTest" />
            </classes>
        </test>  
        <test name = "Search Elysia">
            <parameter name = "Elysia_id" value="3"/>
            <parameter name = "Elysia_name" value="Elysia"/>
