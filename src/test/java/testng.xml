            <classes>
                <class name = "org.teamswt301.dirverdatabasedata.dao.test.TestCRUDFunctionOnCharacterDatabase" />
            </classes>
        </test>
        <test name = "DAO">
            <classes>
                <class name = "org.teamswt301.dirverdatabasedata.dao.test.DAOTesting" />
            </classes>
        </test>
        <test name = "CRUDTest">
            <classes>
                <class name = "org.teamswt301.dirverdatabasedata.dao.test.CharacterCRUDTest" />
